  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kufu/eslint-config-smarthr.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "SmartHR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kufu/eslint-config-smarthr/issues"
  },
  "homepage": "https://github.com/kufu/eslint-config-smarthr#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "vitest": "^29.7.0",
    "@types/react": "^19.0.1",
    "jest": "^29.7.0",
    "react": "^19.0.0",
    "standard-version": "^9.3.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "react": "^16.8.6 || ^17.0.0 || ^18.0.0 || ^19.0.0",
